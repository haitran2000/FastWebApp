<?xml version="1.0" encoding="iso-8859-1"?>
<atscada-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" at-title="ATSCADA" at-brand="ATSCADA" at-author="ATSCADA LAB" at-dark-mode="true">
  <at-component-name>NewApplication</at-component-name>
  <at-component-description>DIGITAL TRANSFORMATION</at-component-description>
  <atscada-components>
    <atscada-assembly at-value="ATSCADAWebApp.Core.ATSCADAView, ATSCADAFastWeb.Core, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null">
      <atscada-view at-content="Home" at-location="home" at-icon="home" at-roles="Admin, Operator" at-interval="1000" at-max-number-of-write="10" at-timeout="3000">
        <at-component-name>NewView</at-component-name>
        <at-component-description>ATSCADA Fast Web Component</at-component-description>
        <atscada-components>
          <atscada-assembly at-value="ATSCADAWebApp.Core.ATSCADARow, ATSCADAFastWeb.Core, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null">
            <atscada-row>
              <at-component-name>NewRow</at-component-name>
              <at-component-description>ATSCADA Fast Web Component</at-component-description>
              <atscada-components>
                <atscada-assembly at-value="ATSCADAWebApp.Core.ATSCADAColumn, ATSCADAFastWeb.Core, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null">
                  <atscada-column at-grid-column="col-sm-12">
                    <at-component-name>NewColumn</at-component-name>
                    <at-component-description>ATSCADA Fast Web Component</at-component-description>
                    <atscada-components>
                      <atscada-assembly at-value="ATSCADAWebApp.Component.DataReporter.ATSCADADataReporter, ATSCADAFastWeb.Component, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null">
                        <atscada-data-reporter at-content="Data Reporter" at-connection="haiyenhi_hanoi" at-table-name="storage" at-timeout="30000" at-grid-column="col-sm-12">
                          <at-component-name>NewDataReporter</at-component-name>
                          <at-component-description>DataReporter - ATSCADA Web Component</at-component-description>
                          <atscada-data-reporter-item at-name="Value" at-alias="T_KT1" at-color="#008080"></atscada-data-reporter-item>
                          <atscada-data-reporter-item at-name="Value2" at-alias="H_KT1" at-color="#008080"></atscada-data-reporter-item>
                          <atscada-data-reporter-item at-name="Value3" at-alias="T_KT2" at-color="#008080"></atscada-data-reporter-item>
                          <atscada-data-reporter-item at-name="Value4" at-alias="H_KT2" at-color="#008080"></atscada-data-reporter-item>
                        </atscada-data-reporter>
                      </atscada-assembly>
                    </atscada-components>
                  </atscada-column>
                </atscada-assembly>
              </atscada-components>
            </atscada-row>
          </atscada-assembly>
        </atscada-components>
      </atscada-view>
    </atscada-assembly>
  </atscada-components>
  <at-authentication at-connection="haiyenhi_hanoi" at-table-name="useraccount" at-required="true"></at-authentication>
  <at-service at-address="localhost" at-port="8010"></at-service>
</atscada-app>